    jcr:description="Responsive Template for Perficient Digital."
    jcr:primaryType="cq:Template"
    jcr:title="Perficient Digital Responsive Template"
    allowedPaths="[/content(/.*)?]"
    ranking="{Long}2">
    <jcr:content
        cq:designPath="/etc/designs/digital"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="digital/components/structure/page-default/responsive">
        <grid-4-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"/>
        <grid-8-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"/>
        <grid-12-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"/>
    </jcr:content>
</jcr:root>
